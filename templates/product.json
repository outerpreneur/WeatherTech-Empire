            "collapsible_tab_text": "<div class=\"variant-metafields-section\">\n{% assign selected_variant = product.selected_or_first_available_variant %}\n{% if selected_variant %}\n  {% assign metafield_value = selected_variant.metafields.car.make | metafield_tag %}\n  <p>Make: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.car.model | metafield_tag %}\n  <p>Model: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.car.year | metafield_tag %}\n  <p>Year: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.custom.variant_identifier | metafield_tag %}\n  <p>Placement: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.car.additional_identifier | metafield_tag %}\n  <p>Additional identifier: {{ metafield_value }}<\/p>\n\n<p>Transmission: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.custom.variant_identifier | metafield_tag %}\n  <p>Transmission: {{ metafield_value }}<\/p>\n{% assign metafield_value = selected_variant.metafields.car.transmission | metafield_tag %}\n  <p>Additional identifier: {{ metafield_value }}<\/p>\n{% endif %}\n<\/div>\n\n\n\n<button style=\"padding: 5px 15px; margin-top: 10px;\"><a href=\"https:\/\/weathertechch.com\/pages\/product-request-form\" style=\"text-decoration: none;\">I can't find my vehicle<\/a><\/button>"
