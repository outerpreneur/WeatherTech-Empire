{
  "sections": {
    "main": {
      "type": "static-product-compare",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "text": "Product information"
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "vendor": {
          "type": "vendor",
          "disabled": true,
          "settings": {
          }
        },
        "inventory-status": {
          "type": "availability",
          "settings": {
            "inventory_display": "hidden",
            "inventory_transfer_notice": true
          }
        },
        "rating": {
          "type": "rating",
          "settings": {
          }
        },
        "heading-2": {
          "type": "heading",
          "settings": {
            "text": "Specifications"
          }
        },
        "dynamic-comparison-2": {
          "type": "dynamic_comparison",
          "settings": {
            "title": "Make",
            "text": "{% assign selected_variant = product.selected_or_first_available_variant %}\n  {% if selected_variant %}\n    {% assign metafield_value = selected_variant.metafields.car.make | metafield_tag %} {{ metafield_value }}<\/p>\n{% endif %}\n<\/div>"
          }
        },
        "dynamic-comparison": {
          "type": "dynamic_comparison",
          "disabled": true,
          "settings": {
            "title": "Model",
            "text": ""
          }
        },
        "dynamic-comparison-3": {
          "type": "dynamic_comparison",
          "disabled": true,
          "settings": {
            "title": "Year",
            "text": ""
          }
        }
      },
      "block_order": [
        "heading",
        "description",
        "vendor",
        "inventory-status",
        "rating",
        "heading-2",
        "dynamic-comparison-2",
        "dynamic-comparison",
        "dynamic-comparison-3"
      ],
      "settings": {
        "filter_groups": "expand_first"
      }
    },
    "dynamic-highlights-banner": {
      "type": "dynamic-highlights-banner",
      "blocks": {
        "highlight": {
          "type": "highlight",
          "settings": {
            "icon": "icon-delivery",
            "title": "Free Shipping",
            "text": "<p>When you spend CHF 200+<\/p>",
            "link": ""
          }
        },
        "highlight-2": {
          "type": "highlight",
          "disabled": true,
          "settings": {
            "icon": "icon-cell-phone",
            "title": "Give Us A Call",
            "text": "<p>+1 (201) 555-5555<\/p>",
            "link": ""
          }
        },
        "highlight-3": {
          "type": "highlight",
          "settings": {
            "icon": "icon-chat-alternate",
            "title": "Chat With Us",
            "text": "<p>Get in touch with our support team<\/p>",
            "link": ""
          }
        },
        "highlight-4": {
          "type": "highlight",
          "disabled": true,
          "settings": {
            "icon": "icon-pin",
            "title": "Locations",
            "text": "<p>Find a store near you<\/p>",
            "link": ""
          }
        }
      },
      "block_order": [
        "highlight",
        "highlight-2",
        "highlight-3",
        "highlight-4"
      ],
      "settings": {
        "section_spacing": true,
        "section_width": "full-width",
        "icon_layout": "center",
        "color": "#4d4d4d",
        "icon_color": "#cc1313",
        "background_color": "rgba(0,0,0,0)",
        "mobile_layout": "slider"
      }
    }
  },
  "order": [
    "main",
    "dynamic-highlights-banner"
  ]
}
